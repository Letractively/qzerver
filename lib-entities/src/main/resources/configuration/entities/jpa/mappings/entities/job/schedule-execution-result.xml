<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings
        xmlns="http://java.sun.com/xml/ns/persistence/orm"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
        version="2.0">

    <entity name="ScheduleExecutionResult" class="org.qzerver.model.domain.job.ScheduleExecutionResult">
        <table name="qzerver_schedule_execution_result"/>
        <attributes>
            <id name="id">
                <column name="id" nullable="false"/>
            </id>
            <basic name="succeed">
                <column name="succeed" nullable="false"/>
            </basic>
            <basic name="created">
                <column name="created" nullable="false"/>
            </basic>
            <version name="version"/>
            <many-to-one name="execution" fetch="LAZY" optional="false">
                <join-column name="scheduler_execution_id" referenced-column-name="id" nullable="false"/>
            </many-to-one>
            <one-to-one name="node" fetch="LAZY" optional="false">
                <join-column name="scheduler_node_id" referenced-column-name="id" nullable="false"/>
            </one-to-one>
        </attributes>
    </entity>

</entity-mappings>