            </column>
        </createTable>
        <addPrimaryKey
                tableName="QRTZ_SCHEDULER_STATE" columnNames="SCHED_NAME,INSTANCE_NAME"
                constraintName="QRTZ_SCHEDULER_STATE_PK"/>

        <!-- QRTZ_LOCKS -->
        <createTable tableName="QRTZ_LOCKS">
