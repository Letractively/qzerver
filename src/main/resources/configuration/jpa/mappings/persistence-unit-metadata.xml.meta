<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings
        xmlns="http://java.sun.com/xml/ns/persistence/orm"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
        version="2.0">

    <persistence-unit-metadata>
        <xml-mapping-metadata-complete/>
        <persistence-unit-defaults>
            <access>PROPERTY</access>
            <cascade-persist/>
        </persistence-unit-defaults>
    </persistence-unit-metadata>

    <!--
        Hibernate metamodel generator would not work without <xml-mapping-metadata-complete/> tag. But Intellij IDEA
        doesn't navigate throught bean with the tag. So we have to different metadata description - one (main) for IDEA,
        and other (this) for metadata generator.
    -->

</entity-mappings>